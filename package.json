{
  "name": "Allok8",
  "version": "1.0.0",
  "engines": {
    "node": ">=8.3.0"
  },
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "start": "node server/index.js",
    "build": "NODE_ENV=production webpack --env.mode production --config ./webpack.config.js ",
    "client": "NODE_ENV=development webpack-dev-server --env.mode development --hot",
    "server": "NODE_ENV=development env-cmd .env nodemon server/index.js",
    "dev": "NODE_ENV=development concurrently \"npm run server\" \"npm run client\"",
    "dev-hot": "NODE_ENV=development npm run server & webpack-dev-server --env.mode development --open --hot --inline --progress --colors --watch --content-base ./",
    "test": " NODE_ENV=test env-cmd .env jest --no-cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "node-pg-migrate",
    "prod-analyze": "NODE_ENV=production webpack --env.presets analyze",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "prod:debug": "npm run debug --mode production",
    "dev:debug": "npm run debug --mode development"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/server/controllers/dbController/tests/",
      "/server/controllers/curlController/tests/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.12.13",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "css-loader": "^3.1.0",
    "dedent": "^0.7.0",
    "env-cmd": "^8.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "jest": "^25.5.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-pg-migrate": "^5.9.0",
    "nodemon": "^2.0.3",
    "pg-format": "^1.0.4",
    "postcss-loader": "^3.0.0",
    "prettier": "2.1.2",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.1",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "@nivo/circle-packing": "^0.61.0",
    "@nivo/line": "^0.61.1",
    "@trendmicro/react-sidenav": "^0.4.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "pg": "^8.0.3",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hooks": "^1.0.1",
    "react-modal": "^3.11.2",
    "react-router-dom": "^5.1.2"
  }
}
